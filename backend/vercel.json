{
    "version": 2,
    "builds": [
        {
            "src": "api/auth.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/health.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/health",
            "dest": "/api/health.js"
        }
    ]
}